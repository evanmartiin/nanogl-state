{
  "name": "nanogl-state",
  "version": "0.1.2",
  "description": "Efficient webgl context state management",
  "main": "state.js",
  "scripts": {
    "test": "npm run build && jshint src/*.js && karma start",
    "build": "uglifyjs --screw-ie8 -m -c warnings=false -b -o config.js -- src/config.js && uglifyjs --screw-ie8 -m -c warnings=false -b -o stack.js -- src/stack.js" ,
    "gztest": "uglifyjs --screw-ie8 -m -c warnings=false -o gztest.js -- src/config.js src/stack.js state.js && gzip -k -9 gztest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plepers/nanogl-state.git"
  },
  "keywords": [
    "webgl nanogl"
  ],
  "author": "Pierre Lepers",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/plepers/nanogl-state/issues"
  },
  "homepage": "https://github.com/plepers/nanogl-state#readme",
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jshint": "^2.9.1-rc1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.3.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "stringify": "^3.2.0",
    "uglify-js": "^2.6.1"
  }
}
